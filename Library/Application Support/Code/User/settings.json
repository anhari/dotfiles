  "vim.disableExtension": false,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    }
  ],
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["z", "g"],
      "commands": ["cSpell.addWordToUserDictionary"]
    },
    {
      "after": ["2", "5", "ctrl+e"],
      "before": ["ctrl+e"]
    },
    {
      "after": ["2", "5", "ctrl+y"],
      "before": ["ctrl+y"]
    },
    {
      "after": ["g", "j"],
      "before": ["j"]
    },
    {
      "after": ["g", "k"],
      "before": ["k"]
    },
    {
      "after": ["^"],
      "before": ["0"]
    },
    {
      "before": ["<leader>", "d", "d"],
      "commands": ["todo.toggleDone"]
    },
    {
      "before": ["<leader>", "d", "c"],
      "commands": ["todo.toggleCancelled"]
    },
    {
      "before": ["leader", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["vscode-test.runLineTests"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["vscode-test.runLastTests"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["vscode-test.runFileTests"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["vscode-test.runAllTests"]
    },
    {
      "before": ["<leader>", "u"],
      "commands": ["vscode-test.runAllUnitTests"]
    },
    {
      "before": ["<leader>", "v", "v"],
      "commands": [
        {
          "command": "expandLineSelection"
        },
        {
          "command": "workbench.action.terminal.runSelectedText"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": [
        {
          "args": {
            "text": "clear\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        },
        {
          "args": {
            "text": "g diff\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        },
        "workbench.action.terminal.focus"
      ]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": [
        {
          "args": {
            "text": "clear\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        },
        {
          "args": {
            "text": "g status\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": [
        {
          "args": {
            "text": "clear\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        },
        {
          "args": {
            "text": "g log\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        }
      ]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": [
        {
          "args": {
            "text": "clear\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        },
        {
          "args": {
            "text": "g browse\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        }
      ]
    },
    {
      "before": ["<leader>", "b", "i"],
      "commands": [
        {
          "args": {
            "text": "clear\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        },
        {
          "args": {
            "text": "bundle install\r"
          },
          "command": "workbench.action.terminal.sendSequence"
        }
      ]
    },
    {
      "before": ["]", "q"],
      "commands": ["search.action.focusNextSearchResult"]
    },
    {
      "before": ["[", "q"],
      "commands": ["search.action.focusPreviousSearchResult"]
    },
    {
      "before": ["<leader>", "i"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "after": [],
      "args": "mocha:spec",
      "before": ["<leader>", "f", "t"],
      "command": "workbench.action.tasks.runTask"
    }
  ],
