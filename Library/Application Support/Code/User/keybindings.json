// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "ctrl+j",
    "command": "workbench.action.navigateDown"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.navigateUp"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.new",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+enter",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "terminalFocus"
  },
  {
    "key": "shift+cmd+f",
    "command": "-workbench.action.findInFiles"
  },
  {
    "key": "alt+\\",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "shift+cmd+f5",
    "command": "-workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "shift+cmd+r",
    "command": "workbench.action.debug.selectandstart"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.openRecent"
  },
  {
    "key": "ctrl+r",
    "command": "-workbench.action.openRecent"
  },
  {
    "key": "cmd+k cmd+o",
    "command": "outline.focus"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.tasks.runTask",
    "args": "mocha:spec"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.tasks.runTask",
    "args": "mocha:file"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm"
  },
  {
    "key": "cmd+1",
    "command": "workbench.action.editorLayoutSingle"
  },
  {
    "key": "shift+cmd+f",
    "command": "-workbench.view.search",
    "when": "!searchViewletVisible"
  },
  {
    "key": "shift+cmd+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "cmd+[",
    "command": "editor.fold",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+cmd+[",
    "command": "-editor.fold",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.tasks.runTask",
    "args": "Gitsh"
  },
  {
    "key": "alt+p",
    "command": "workbench.action.quickOpenTerm"
  },
  {
    "key": "alt+\\",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+\\",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus"
  },
  {
    "key": "shift+cmd+h",
    "command": "workbench.action.tasks.restartTask"
  },
  {
    "key": "shift+ctrl+h",
    "command": "workbench.action.tasks.terminate"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.tasks.reRunTask"
  },
  {
    "key": "ctrl+shift+.",
    "command": "itunes.dislikeTrack"
  },
  {
    "key": "ctrl+shift+,",
    "command": "itunes.likeTrack"
  },
  {
    "key": "ctrl+shift+l",
    "command": "workbench.action.terminal.runSelectedText"
  },
  {
    "key": "ctrl+shift+a",
    "command": "workbench.action.terminal.runActiveFile"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.reloadWindow"
  },
  {
    "key": "ctrl+shift+c",
    "command": "workbench.files.action.collapseExplorerFolders"
  },
  {
    "key": "ctrl+shift+f",
    "command": "workbench.action.replaceInFiles"
  },
  {
    "key": "shift+cmd+h",
    "command": "-workbench.action.replaceInFiles"
  },
  {
    "key": "cmd+i",
    "command": "editor.action.insertSnippet"
  },
  {
    "key": "ctrl+shift+down",
    "command": "workbench.action.positionPanelBottom"
  },
  {
    "key": "ctrl+shift+right",
    "command": "workbench.action.positionPanelRight"
  },
  {
    "key": "ctrl+shift+o",
    "command": "vscode-test.openAlternateFile"
  },
  {
    "key": "cmd+ctrl+shift+o",
    "command": "vscode-test.openAlternateFileInVerticalSplit"
  },
  {
    "key": "cmd+k l",
    "command": "workbench.action.moveEditorToRightGroup"
  },
  {
    "key": "cmd+k h",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "cmd+k j",
    "command": "workbench.action.moveActiveEditorGroupDown"
  },
  {
    "key": "cmd+k down",
    "command": "-workbench.action.moveActiveEditorGroupDown"
  },
  {
    "key": "cmd+k down",
    "command": "-workbench.action.moveActiveEditorGroupDown"
  },
  {
    "key": "cmd+k k",
    "command": "workbench.action.moveActiveEditorGroupUp"
  },
  {
    "key": "cmd+k up",
    "command": "-workbench.action.moveActiveEditorGroupUp"
  },
  {
    "key": "shift+cmd+a",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "shift+5",
    "command": "editor.emmet.action.matchTag",
    "when": "editorTextFocus && vim.active && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+shift+cmd+y",
    "command": "yarn-script.runScript"
  },
  {
    "key": "cmd+r shift+r",
    "command": "-yarn-script.runScript"
  },
  {
    "key": "shift+cmd+t",
    "command": "todo.open"
  },
  {
    "key": "alt+l",
    "command": "vscode-test.runLastTests"
  },
  {
    "key": "alt+t",
    "command": "vscode-test.runFileTests"
  },
  {
    "key": "ctrl+shift+s",
    "command": "workbench.view.scm"
  },
  {
    "key": "ctrl+`",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  }
]
